=stylesheet_link_tag 'planilha'

#planilha
  %table
    %thead
      %tr
        %td
        -@horas.each do |hora|
          %td= "%02d:00" % hora
        %td
    
    %tfoot
      %tr
        %td
        -@horas.each do |hora|
          %td= "%02d:00" % hora
        %td
  
    %tbody
      -@dias.each do |dia|
        %tr
          %td= dia.strftime "%a/%d/%b"
          -@horas.each do |hora|
            %td= celula dia, hora
          %td= dia.strftime "%a/%d/%m"
  
